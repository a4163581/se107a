  510  * take([1, 2, 3])
  511   * // => [1]
  512   *
  513   * take([1, 2, 3], 2)
  514   * // => [1, 2]
  515   *
  516   * take([1, 2, 3], 5)
  517   * // => [1, 2, 3]
  518   *
  519   * take([1, 2, 3], 0)
  520   * // => []
  521   */
  522  function take(array, n=1) {
  523      return []
  524    }
  525    return slice(array, 0, n < 0 ? 0 : n)
  526  }
  527  export default take
